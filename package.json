{
  "name": "subh-stories-accounts",
  "version": "1.0.0",
  "description": "Modern offline desktop accounting software for Subh Stories video editing business",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "dist": "electron-builder --win"
  },
  "keywords": ["electron", "accounting", "video editing", "invoice"],
  "author": "Subh Stories",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@mui/material": "^5.14.0",
    "@mui/icons-material": "^5.14.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "better-sqlite3": "^9.0.0",
    "jspdf": "^2.5.1",
    "recharts": "^2.8.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "electron": "^27.0.0",
    "electron-builder": "^24.6.0",
    "@babel/core": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "babel-loader": "^9.1.0",
    "html-webpack-plugin": "^5.5.0"
  },
  "build": {
    "appId": "com.subhstories.accounts",
